package DeckOfCards;

public enum Suit {

	HEARTS("Hearts",1),
	SPADES("Spades",2),
	DIAMONDS("Diamonds",3),
	CLUBS("Clubs",4);
	
	private int value;
	private String suitName;
	
	private Suit(String suit, int value){
		suit = suitName;
		value = value;
	}

	public int getValueOfSuit() {
		return value;
	}

	public void setValue(int value) {
		this.value = value;
	}

	public String getSuitName() {
		return suit;
	}

	public void setSuit(String suit) {
		this.suit = suit;
	}
	
}
